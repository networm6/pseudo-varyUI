<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xiaoheng.flayout"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="xiaoheng.flayout.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="xiaoheng.flayout.Add"
            android:theme="@style/Add" />
        <activity
            android:name="xiaoheng.flayout.My"
            android:theme="@style/My" />
        <activity
            android:name="xiaoheng.flayout.About"
            android:theme="@style/My" />
        <activity
            android:name="xiaoheng.flayout.Register"
            android:theme="@style/Register" />
        <activity
            android:name="xiaoheng.flayout.Welcome"
            android:theme="@style/Welcome" />
        <activity
            android:name="xiaoheng.flayout.Login"
            android:theme="@style/Welcome" />
    </application>

</manifest>